{
    "name": "daothanh/webvicms",
    "type": "project",
    "description": "The Webvi CMS.",
    "keywords": [
        "framework",
        "laravel",
        "laravel cms"
    ],
    "license": "MIT",
    "require": {
        "php": "^7.2",
        "artesaos/seotools": "^0.16.0",
        "astrotomic/laravel-translatable": "^11.1",
        "fideloper/proxy": "^4.0",
        "intervention/image": "^2.4",
        "laravel/framework": "^6.2",
        "laravel/slack-notification-channel": "^2.0",
        "laravel/socialite": "^4.3",
        "laravel/tinker": "^2.0",
        "laravelcollective/html": "^6.0",
        "maatwebsite/laravel-sidebar": "~2.1",
        "nwidart/laravel-modules": "^6.0",
        "spatie/laravel-permission": "^3.6.0",
        "spatie/laravel-sitemap": "^5.*",
        "tightenco/ziggy": "^0.8.1",
        "ext-pdo": "*"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.2",
        "barryvdh/laravel-ide-helper": "^2.6",
        "facade/ignition": "^1.4",
        "fzaninotto/faker": "^1.4",
        "mockery/mockery": "^1.0",
        "nunomaduro/collision": "^3.0",
        "phpunit/phpunit": "^8.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Modules\\": "modules/"
        },
        "classmap": [
            "database/seeds",
            "database/factories"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ]
    }
}
